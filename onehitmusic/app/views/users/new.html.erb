<h1>New User</h1>

<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :firstname %><br>
    <%= f.text_field :firstname %>
  </p>
 
  <p>
    <%= f.label :lastname %><br>
    <%= f.text_field :lastname %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :birthdate %><br>
    <%= f.text_field :birthdate %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>